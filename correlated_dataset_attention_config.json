{
    "embedding_dim": 64,
    "loss_type": "bce",
    
    "user_num_attention_layers": 2,
    "user_num_heads": 8,
    "user_dropout": 0.1,
    "user_use_cls_token": true,
    "user_use_layer_norm": true,
    "user_use_simple_fusion": false,
    
    "item_num_attention_layers": 2,
    "item_num_heads": 8,
    "item_dropout": 0.1,
    "item_use_simple_fusion": false,
    
    "interaction_num_attention_layers": 2,
    "interaction_num_heads": 8,
    "interaction_dropout": 0.1,
    "interaction_use_simple_fusion": false,
    
    "classifier_hidden_dims": [128, 64],
    "classifier_dropout": 0.1,
    
    "text_encoder_config": {
      "model_name": "glove-wiki-gigaword-50",
      "aggregation_strategy": "mean",
      "embedding_dim": 64,
      "num_text_fields": 2
    },
    
    "categorical_encoder_config": {
      "aggregation_strategy": "separate_concat",
      "hash_vocab_size": 4096,
      "embedding_dim": 64,
      "num_categorical_fields": 1,
      "mlp_hidden_dims": []
    },
    
    "continuous_encoder_config": {
      "embedding_dim": 64,
      "hidden_dims": [64],
      "dropout": 0.0,
      "normalize": true
    },
    
    "image_encoder_config": null,
    
    "temporal_encoder_config": {
      "enable_item_lookup": true,
      "aggregation_strategy": "mean_pooling",
      "output_dim": 64
    },
    
    "train_split": 0.8,
    "batch_size": 32,
    "negative_sampling_ratio": 1.0,
    "seed": 42,
    
    "num_epochs": 10,
    "learning_rate": 0.001,
    "optimizer_type": "adam",
    "scheduler_type": "cosine",
    "print_every": 1,
    "model_name": "attention_fusion_model"
  }

  